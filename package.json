{
  "name": "rebirth",
  "version": "1.0.1",
  "description": "Electron main process application",
  "main": "dist/main.js",
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "start": "chcp 65001 && electron .",
    "watch": "vite build --watch",
    "lint": "eslint -c .eslintrc --ext .ts ./src",
    "pack": "electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win --x64",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:mac-x64": "npm run build && electron-builder --mac --x64",
    "dist:mac-arm64": "npm run build && electron-builder --mac --arm64",
    "dist:mac-universal": "npm run build && electron-builder --mac --universal",
    "dist:linux": "npm run build && electron-builder --linux --x64",
    "postinstall": "electron-builder install-app-deps",
    "analyze": "node scripts/analyze-bundle.js",
    "clean": "node scripts/clean.js",
    "clean:build": "npm run clean && npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@electron/notarize": "^2.5.0",
    "@types/ali-oss": "^6.16.11",
    "@types/better-sqlite3": "^7.6.4",
    "@types/iconv-lite": "^0.0.1",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "electron": "^29.4.6",
    "electron-builder": "^24.9.1",
    "electron-icon-builder": "^2.0.1",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.37.0",
    "nodemon": "^3.1.10",
    "prebuild-install": "^7.1.3",
    "sharp": "^0.34.3",
    "terser": "^5.43.1",
    "typescript": "^5.0.3",
    "vite": "^6.3.5",
    "vite-plugin-electron": "^0.29.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.821.0",
    "@aws-sdk/s3-request-presigner": "^3.821.0",
    "@node-rs/jieba": "^2.0.1",
    "@types/archiver": "^6.0.3",
    "@types/jsdom": "^21.1.7",
    "@types/ws": "^8.18.1",
    "@types/yauzl": "^2.10.3",
    "ali-oss": "^6.23.0",
    "ansis": "^4.0.0",
    "archiver": "^7.0.1",
    "better-sqlite3": "^11.10.0",
    "class-validator": "^0.14.2",
    "cli-color": "^2.0.4",
    "cos-nodejs-sdk-v5": "^2.15.1",
    "dotenv": "^16.5.0",
    "electron-log": "^5.4.1",
    "electron-store": "^8.2.0",
    "electron-updater": "^6.6.2",
    "exif-reader": "^1.0.3",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "iconv-lite": "^0.6.3",
    "idna-uts46": "^1.1.0",
    "jsdom": "^26.1.0",
    "mammoth": "^1.6.0",
    "node-machine-id": "^1.1.12",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^3.11.174",
    "pptx2json": "^0.0.10",
    "reflect-metadata": "^0.1.13",
    "sql-highlight": "^6.0.0",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.15",
    "uuid": "^11.1.0",
    "webdav": "^5.8.0",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5",
    "yauzl": "^3.2.0"
  },
  "build": {
    "appId": "com.rebirth.app",
    "publish": [
      {
        "provider": "generic",
        "url": "https://your-update-server.com"
      }
    ],
    "productName": "Rebirth",
    "npmRebuild": false,
    "copyright": "Copyright Â© 2024",
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "icon": "src/assets/icons/windows/icon.ico",
    "files": [
      "dist/**/*",
      "package.json",
      "node_modules/**/*",
      "!src/**/*",
      "!test/**/*",
      "!**/*.ts",
      "!**/*.map",
      "!node_modules/**/*.d.ts",
      "!node_modules/**/*.map",
      "!node_modules/**/test/**/*",
      "!node_modules/**/tests/**/*",
      "!node_modules/**/example/**/*",
      "!node_modules/**/examples/**/*",
      "!node_modules/**/docs/**/*",
      "!node_modules/**/README.md",
      "!node_modules/**/CHANGELOG.md",
      "!node_modules/**/LICENSE",
      "!node_modules/**/license"
    ],
    "extraResources": [
      {
        "from": "renderer/dist",
        "to": "renderer/dist"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "src/assets/icons/windows/icon.ico",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "publisherName": "wenjian zhang"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Rebirth",
      "deleteAppDataOnUninstall": false
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "src/assets/icons/macos/icon.icns",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "category": "public.app-category.utilities",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build/entitlements.mac.plist",
      "entitlementsInherit": "build/entitlements.mac.plist",
      "identity": "wenjian zhang (48N382RB7X)",
      "extendInfo": {
        "ElectronTeamID": "48N382RB7X"
      },
      "notarize": false
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "src/assets/icons/linux/icon-512x512.png",
      "artifactName": "${productName}-${version}.${ext}",
      "category": "Utility"
    },
    "asar": true,
    "includeSubNodeModules": false,
    "electronDownload": {
      "mirror": "https://npmmirror.com/mirrors/electron/"
    },
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*",
      "node_modules/@node-rs/jieba/**/*",
      "node_modules/@node-rs/**/*"
    ],
    "removePackageScripts": true,
    "removePackageKeywords": true,
    "extraMetadata": {
      "main": "dist/main.js"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
